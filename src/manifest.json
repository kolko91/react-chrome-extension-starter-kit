{
  "manifest_version": 2,
  "name": "RCE",
  "description": "React, Chrome extension",
  "version": "0.0.1",
  "icons": {
    "64": "assets/icon-128.png",
    "128": "assets/icon-256.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "<all_urls>"
  ],
  "background": {
   "scripts": [
     "background.bundle.js"
   ]
 },
  "browser_action": {
    "default_icon": "assets/icon-128.png",
    "default_title": "RCE",
    "default_popup": "popup.html"
  }

}
